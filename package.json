{
  "name": "yo-bot",
  "version": "0.0.0",
  "description": "Easily extendable slackbot for greeting new team members in your Slack Group",
  "main": "lib/yo-bot.js",
  "scripts": {
    "clean:db": "rm data/yo-bot.db && npm run create:db",
    "preclean:install": "rm -rf node_modules",
    "clean:install": "npm install",
    "create:db": "sqlite3 data/yo-bot.db \"create table info(name text, val text);create table users(uid text);\"",
    "preinstall": "npm run clean:db",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "slack",
    "bot"
  ],
  "author": "datatypevoid",
  "license": "MIT",
  "dependencies": {
    "q": "^1.4.1",
    "slackbots": "^0.5.1",
    "sqlite3": "^3.1.4"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1"
  }
}
